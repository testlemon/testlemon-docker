    "version": "0.2.0",
    "configurations": [
    {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/Debug/net9.0/console.dll",
            "args": [
                "--collections",
                "https://api.apis.guru/v2/openapi.yaml",
                "--output",
                "output.json"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
    ]
}